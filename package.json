{
  "name": "test-element",
  "version": "1.0.0",
  "description": "Flood Element test script",
  "private": true,
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "useTabs": true,
    "tabWidth": 2,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "@flood/element": "^1.2.3",
    "@flood/element-cli": "^1.2.3",
    "assert": "*",
    "faker": "*",
    "prettier": "*"
  },
  "devDependencies": {
    "@types/assert": "*",
    "@types/faker": "*",
    "ts-loader": "^7.0.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "element": {
    "testMatch": [
      "**/*.perf.[tj]s"
    ]
  },
  "scripts": {
    "test": "node ./src/app.runner.js -s",
    "test:all": "node ./src/app.runner.js -a",
    "generate": "node ./src/app.runner.js -g"
  }
}
